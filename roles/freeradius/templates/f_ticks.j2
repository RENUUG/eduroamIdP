# -*- text -*-
##
##  f_ticks -- Configuration for f_ticks logging
##
##  Per https://wiki.geant.org/display/H2eduroam/freeradius-sp
##
linelog f_ticks {
       filename = syslog
       #syslog_facility = local0
       #syslog_severity = info
       format = ""
       reference = {% raw %}"f_ticks.%{%{reply:Packet-Type}:-format}"{% endraw %}
       f_ticks {
                Access-Accept = "F-TICKS/eduroam/1.0#REALM=%{Realm}#VISCOUNTRY={{ country_code }}#VISINST=%{Operator-Name}#CSI=%{Calling-Station-Id}#RESULT=OK#"
                Access-Reject = "F-TICKS/eduroam/1.0#REALM=%{Realm}#VISCOUNTRY={{ country_code }}#VISINST=%{Operator-Name}#CSI=%{Calling-Station-Id}#RESULT=FAIL#"
            }
}
